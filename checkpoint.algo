i: entier
x: entier
nbr_caractere: entier
nbr_voyelle : entier
ch: STRING
BEGIN
    i=0
x= long(ch)
Répeter
    Ecrire ("Donner votre chaine")
    lire(ch)
jusqu'a (long(ch)<>0)&(ch[long(ch)]=".")

nbr_caractere=0
nbr_voyelle=0
